r leftPos = (w-popW)/2;\n	var topPos = (h-popH)/2;\n	\n	var atrib='titlebar=no,resizable=no,menubar=no,toolbar=no,scrollbars='+scroll+',status=no,width=' + popW + ',height=' + popH + ',top=' + topPos + ',left=' + leftPos;\n	var winname='PoPup'+(document.forms[0].DocId==undefined?\"\":document.forms[0].DocId.value);\n	\n	return window.open(url,winname,atrib);\n}\n\nfunction PoPupCenterScroll(url,popW,popH)\n{\n	\n	var w=screen.availWidth;\n	var h=screen.availHeight;\n\n	var leftPos = (w-popW)/2;\n	var topPos = (h-popH)/2;\n	\n	var atrib='titlebar=no,resizable=no,menubar=no,toolbar=no,scrollbars=yes,status=no,width=' + popW + ',height=' + popH + ',top=' + topPos + ',left=' + leftPos;\n	var winname='PoPup'+(document.forms[0].DocId==undefined?\"\":document.forms[0].DocId.value);\n	\n	return window.open(url,winname,atrib);\n}\n\n/*****************************************************************************************************/\n//funcionalidades con string\n/*****************************************************************************************************/\nfunction enter()\n{\n	return String.fromCharCode(13);\n}\n\nfunction Left(str, subStr)\n{\n	n=str.length\n	for (var i=0; i<str.length; i++)\n	{      \n     	if (str.substring(i,i+subStr.length) == subStr)\n		{\n         		n=i\n      	}\n   	}\n   	\n	if (n <= 0)\n		return \"\";\n	else if (n > String(str).length)\n		return str;\n	else\n	 	return String(str).substring(0,n);\n}\n\nfunction Right(str, subStr)\n{\n	n=str.length\n	for (var i=0; i<str.length; i++)\n	{      \n     	if (str.substring(i,i+subStr.length) == subStr)\n		{\n			n=n-(i+subStr.length)\n      	}\n   	}\n   	\n	if (n <= 0)\n     	return \"\";\n   	else if (n > String(str).length)\n     	return str;\n    else\n	{\n     	var iLen = String(str).length;\n     	return String(str).substring(iLen, iLen - n);\n	}\n}\n\n//Verifica si un estring se encuentra dentro de un campo multivalor\nfunction StrInFldMultVal(str,fld)\n{\n	for (i=0;i<fld.length;i++)\n		if(fld.options[i].text == str)\n			return true;\n\n	return false;\n}\n\nfunction StrInFldMultValSplit(str,fld)\n{\n	arr=fld.value.split(',');\n	for (i=0;i<arr.length;i++)\n		if(trim(arr[i]) == str)\n			return true;\n\n	return false;\n}\n\n/*****************************************************************************************************/\n//funcionalidades con Divisiones\n/*****************************************************************************************************/\n//Muestra divisiones enumerados\nfunction ShowDivFromTo(DivPrefix, From, To)\n{\n	for (var i=From;i<=To;i++) //{alert('Show='+DivPrefix + i);\n		document.getElementById(DivPrefix + i).style.display=\"block\"; //}\n}\n\n//Oculta divisiones enumerados\nfunction HideDivFromTo(DivPrefix, From, To)\n{\n	for (var i=From;i<=To;i++) //{alert('Hide='+DivPrefix + i);\n		document.getElementById(DivPrefix + i).style.display=\"none\"; //}\n}\n\n//Muestra y Oculta un conjunto de Divisiones\nfunction ToggleShowHideDivs(HideDivs, ShowDivs)\n{\n	for (x = 0; x < ShowDivs.length; x++)\n		document.getElementById(ShowDivs[x]).style.display=\"block\";\n		\n	for (x = 0; x < HideDivs.length; x++)\n		document.getElementById(HideDivs[x]).style.display=\"none\";\n}\n\n/*****************************************************************************************************/\n//funcionalidades con Campos\n/*****************************************************************************************************/\n//Muestra y Oculta un conjunto de Campos\nfunction ShowHideFields(HideFields, ShowFields)\n{\n	for (x = 0; x < ShowFields.length; x++)\n		document.getElementById(ShowFields[x]).style.display=\"block\";\n\n	for (x = 0; x < HideFields.length; x++)\n		document.getElementById(HideFields[x]).style.display=\"none\";\n}\n\nfunction copyfields(fieldsArraySource, fieldsArrayTarget)\n{\n	for (x = 0; x < fieldsArraySource.length; x++)\n	{\n		var ffield = document.getElementsByName(fieldsArraySource[x]).item(0);\n		var sfield = document.getElementsByName(fieldsArrayTarget[x]).item(0);\n\n		if (ffield.nodeName==\"SELECT\")\n			sfield.selectedIndex = ffield.selectedIndex;\n		else if (ffield.nodeName==\"INPUT\")\n			sfield.value = ffield.value;\n	}\n}\n\n//Seteo de campos definidos en un arreglo\nfunction SetFields(fieldsArray)\n{\n\n	for (var x = 0; x < fieldsArray.length; x++)\n	{\n		var e=document.getElementsByName(fieldsArray[x])[0];\n		switch (e.type) \n		{\n			case \"text\":\n					e.value='';\n					break;\n			case \"textarea\":\n					e.value='';\n					break;\n			case \"select-one\":\n					e.selectedIndex = 0;\n					break;\n			case \"select-multiple\":\n					e.selectedIndex =-1;\n					break;\n			case \"checkbox\":\n					p = eval(\"e.form.\" + e.name);\n					for (var r=0; r < p.length; r++)\n						p[r].checked=false;\n					break;\n			case \"radio\":\n					p = eval(\"e.form.\" + e.name);\n					for (var r=0; r < p.length; r++)\n						p[r].checked=false;\n					break;\n			default:\n					break;\n		}		\n	}	\n	\n	fldEnabled(fieldsArray);\n}\n\nfunction fldValida(fieldsArray)\n{\n	//fieldsArray[x]='Nombre Visual#Nombre Campo'\n	for (var x = 0; x < fieldsArray.length; x++)\n	{	\n		a = fieldsArray[x].split(\"#\");\n		var e=document.getElementsByName(a[1])[0];\n		switch (e.type) \n		{\n			case \"text\":\n					if ( trim(e.value) == \"\" )\n					{\n						alert( a[0]+ \" es un dato requerido.\\n\\nPor Favor, Ingrese Informacion\");\n						e.focus();\n						return false;\n					}\n					break;\n			case \"textarea\":\n					if ( trim(e.value) == \"\" )\n					{\n						alert(a[0]+ \" es un dato requerido.\\n\\nPor Favor, Ingrese Informacion\");\n						e.focus();\n						return false;\n					}\n					break;\n			case \"select-one\":\n					if ( e.selectedIndex == 0 )\n					{\n					\n						var valor=trim(e.options[e.selectedIndex].text);\n						if (valor == '' || valor == 'Seleccionar Marca' || valor == 'Seleccionar Modelo')\n						{\n							alert(a[0]+ \" es un dato requerido.\\n\\nPor Favor, Seleccione una Entrada\");\n							e.focus();\n							return false;\n						}\n					}\n					break;\n			case \"select-multiple\":\n					if (e.selectedIndex == -1)\n					{\n						alert(a[0]+ \" es un dato requerido.\\n\\nPor Favor, Seleccione una Entrada\");\n						e.focus();\n						return false;\n					}\n					break;\n			case \"checkbox\":\n					hasChecked = false;\n					p = eval(\"e.form.\" + e.name);\n					for (var r=0; r < p.length; r++)\n					{\n						if ( p[r].checked )\n						{\n							hasChecked = true;\n						}\n					}\n					if (!hasChecked) \n					{\n						alert(a[0]+ \" es un dato requerido.\\n\\nPor Favor, Seleccione una Entrada\");\n						e.focus();\n						return hasChecked;\n					}\n					break;\n			case \"radio\":\n					hasChecked = false;\n					p = eval(\"e.form.\" + e.name);\n						for (var r=0; r < p.length; r++)\n					{\n						if ( p[r].checked )\n						{\n							hasChecked = true;\n						}\n					}\n						if (!hasChecked) \n					{\n						alert(a[0]+ \" es un dato requerido.\\n\\nPor Favor, Seleccione una Entrada\");\n						e.focus();\n						return hasChecked;\n					}\n					break;\n			default:\n					break;\n		}\n	}\n	return true;\n}\n\nfunction fldFocus(strfld)\n{\n	document.getElementById(strfld)[0].focus();\n}\n\nfunction fldDisabled(fieldsArraySource)\n{\n	for (x = 0; x < fieldsArraySource.length; x++)\n	{\n		var sfield = document.getElementsByName(fieldsArraySource[x]).item(0);\n		sfield.setAttribute('disabled', true); \n	}\n	\n}\n\nfunction fldEnabled(fieldsArraySource)\n{\n	for (x = 0; x < fieldsArraySource.length; x++)\n	{\n		var sfield = document.getElementsByName(fieldsArraySource[x]).item(0);\n		sfield.setAttribute('disabled', false); \n	}\n}\n\n\nfunction fldCleanDialogList(fldDiaLis)\n{\n	while (fldDiaLis.length != 0)\n		fldDiaLis.options[0] = null;\n}\nfunction manda(campo){\nwindow.opener.document.DerivaA.value=campo;\nwindow.opener.document.refresh();\n}\n\n/****************************************************\nFuncion para manejar a traves del \"onkeyPress\" solo numeros y solo Char */\nvar nav4 = window.Event ? true : false;\nfunction acceptChar(evt){\n	 // NOTE: Backspace = 8, Enter = 13, '0' = 48, '9' = 57\n	var key = nav4 ? evt.which : evt.keyCode;\n	//alert(key);\n	return (key <= 13 || (key >= 32 && key <= 32) || (key >= 65 && key <= 90) || (key >34 && key < 37) || (key > 208 && key < 210) || (key >=97 && key <=122) || (key > 240 && key < 242));\n}\nfunction acceptNum(evt){\n	 // NOTE: Backspace = 8, Enter = 13, '0' = 48, '9' = 57\n	var key = nav4 ? evt.which : evt.keyCode;\n	return (key <= 13 || (key >= 48 && key <= 57));\n}\n/* Termino de Codigo para \"onkeyPress\"\n/*****************************************************/\n\n\ncheckRutField",
